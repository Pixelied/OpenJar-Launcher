{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "OpenJar Launcher",
    "version": "0.1.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true,
        "save": true
      },
      "shell": {
        "open": true
      },
      "process": {
        "relaunch": true
      }
    },
    "windows": [
      {
        "title": "OpenJar Launcher",
        "width": 1100,
        "height": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://github.com/Pixelied/OpenJar-Launcher/releases/latest/download/latest.json"
      ],
      "pubkey": "RWQHhXdg2WbVaK2xK7oUy76gYUVQ4k+X3XERa81oSDtHX9BIFaOZLaZg"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.github.pixelied.openjarlauncher",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
